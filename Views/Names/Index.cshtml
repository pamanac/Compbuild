@model IEnumerable<Compbuild.Models.Names>

@{
    ViewData["Title"] = "Names Page";
}

<h1 class="text-center">
    Names!
</h1>
<a asp-controller="Names" asp-action="Add" class="btn btn-primary">ADD</a>
<p>
    @if(Model.Count() > 0){
        @foreach(var obj in Model){
            <p>Name: @obj.NAME</p>
        }
    }else{
        <p> No names to display </p>
    }
</p>
